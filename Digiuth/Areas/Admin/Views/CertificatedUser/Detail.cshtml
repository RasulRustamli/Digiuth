@model Certificate
@{
    ViewData["Title"] = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<div class="row mt-5">
    <div class="col-2">
        <h6>FirstName</h6>
    </div>
    <div class="col-4">
        <p> @Model.FirstName</p>
    </div>
</div>
<div class="row mt-2">
    <div class="col-2">
        <h6>LastName</h6>
    </div>
    <div class="col-4">
        <p> @Model.LastName</p>
    </div>
</div>
<div class="row mt-2">
    <div class="col-2">
        <h6>CertificateCode</h6>
    </div>
    <div class="col-4">
        <p> @Model.CertificateCode</p>
    </div>
</div>

<div class="row mt-2">
    <div class="col-2">
        <h6>CourseName</h6>
    </div>
    <div class="col-4">
        <p> @Model.CourseName</p>
    </div>
</div>
<div class="row mt-2">
    <div class="col-2">
        <h6>TeacherName</h6>
    </div>
    <div class="col-4">
        <p> @Model.TeacherName</p>
    </div>
</div>
<div class="row mt-2">
    <div class="col-2">
        <h6>EndDate</h6>
    </div>
    <div class="col-4">
        <p>@Model.EndDate.ToString("dd MMMM yyyy")</p>
    </div>
</div>
<div class="row mt-3">
    <a class="btn btn-dark" >Go Back</a>
</div>

